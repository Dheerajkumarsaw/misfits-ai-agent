    "id": "fastapi-server"
   },
   "source": [
    "## Step 6: Start FastAPI Server"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "start-server"
   },
   "outputs": [],
   "source": [
    "# Start the FastAPI server with ngrok\n",
    "import subprocess\n",
    "import threading\n",
    "import time\n",
    "from pyngrok import ngrok\n",
    "\n",
    "def start_server():\n",
    "    \"\"\"Start the FastAPI server in background\"\"\"\n",
    "    try:\n",
    "        # Start uvicorn server\n",
    "        subprocess.run([\n",
    "            \"python\", \"-m\", \"uvicorn\", \n",
    "            \"api_server:app\", \n",
    "            \"--host\", \"0.0.0.0\", \n",
    "            \"--port\", \"8000\",\n",
    "            \"--reload\"\n",
    "        ])\n",
    "    except Exception as e:\n",
    "        print(f\"Server error: {e}\")\n",
    "\n",
    "# Start server in background\n",
    "print(\"üöÄ Starting FastAPI server...\")\n",
    "server_thread = threading.Thread(target=start_server, daemon=True)\n",
    "server_thread.start()\n",
    "\n",
    "# Wait for server to start\n",
    "time.sleep(5)\n",
    "\n",
    "# Create ngrok tunnel\n",
    "print(\"üåê Creating public URL with ngrok...\")\n",
    "try:\n",
    "    # Create tunnel\n",
    "    public_url = ngrok.connect(8000)\n",
    "    print(f\"\\nüéâ SUCCESS! Your API is now public at:\")\n",
    "    print(f\"üì± Public URL: {public_url}\")\n",
    "    print(f\"\\nüîó Available Endpoints:\")\n",
    "    print(f\"  ‚Ä¢ {public_url}/api/recommendations - Main API\")\n",
    "    print(f\"  ‚Ä¢ {public_url}/api/chat - Chat endpoint\")\n",
    "    print(f\"  ‚Ä¢ {public_url}/api/stats - System stats\")\n",
    "    print(f\"  ‚Ä¢ {public_url}/health/liveness - Health check\")\n",
    "    print(f\"  ‚Ä¢ {public_url} - API documentation\")\n",
    "    \n",
    "except Exception as e:\n",
    "    print(f\"‚ùå Error creating public URL: {e}\")\n",
    "    print(\"üí° Try installing ngrok manually or check your connection\")"
   ]
